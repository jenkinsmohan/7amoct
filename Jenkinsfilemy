pipeline{
    agent any
    stages{
        stage('scm checkout'){
            steps{
                git credentialsId: 'jenkinsmohan', 
                url: 'https://github.com/jenkinsmohan/7amoct',
                branch: 'master'
          }
        }
        stage('Maven Build'){
            steps{
                sh "mvn clean package"
          }
    }

}
}